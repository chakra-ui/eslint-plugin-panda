{
  "name": "eslint-plugin-panda",
  "version": "0.0.1",
  "description": "Eslint plugin for Panda CSS",
  "author": "Abraham Aremu <anubra266@gmail.com>",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r --parallel build",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "lint:eslint-docs": "npm run update:eslint-docs -- --check",
    "update:eslint-docs": "eslint-doc-generator plugin && pnpm run prettier-fix",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "typecheck": "pnpm -r --parallel typecheck"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@typescript-eslint/utils": "^6.19.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@typescript-eslint/rule-tester": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-doc-generator": "^1.6.2",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "prettier": "^3.2.4",
    "vitest": "^1.2.1"
  }
}