{
  "name": "@pandacss/eslint-plugin",
  "version": "0.3.0",
  "description": "Eslint plugin for Panda CSS",
  "author": "Abraham Aremu <anubra266@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "tsup",
    "dev": "tsup --watch",
    "deps": "pnpm up '@pandacss/*' --latest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@pandacss/config": "^0.53.2",
    "@pandacss/generator": "^0.53.2",
    "@pandacss/node": "^0.53.2",
    "@pandacss/shared": "^0.53.2",
    "@typescript-eslint/utils": "^8.21.0",
    "hookable": "^5.5.3",
    "micromatch": "^4.0.8",
    "synckit": "^0.9.0"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.10",
    "typescript": "^5.7.2"
  }
}
